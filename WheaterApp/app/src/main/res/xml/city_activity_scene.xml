<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@layout/activity_city_motion_scene_end"
        motion:constraintSetStart="@layout/activity_city_motion_scene_start">
        <OnSwipe motion:dragDirection="dragUp" />
        <OnClick
            motion:clickAction="transitionToEnd"
            motion:targetId="@+id/city_activity_search_text_view" />
    </Transition>
</MotionScene>